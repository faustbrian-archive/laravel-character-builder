{
    "name": "faustbrian/laravel-character-builder",
    "description": "Character Avatar Builder for Laravel",
    "keywords": [
        "faustbrian",
        "laravel",
        "laravel-character-builder"
    ],
    "homepage": "https://github.com/faustbrian/laravel-character-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@faust.mx",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "endroid/qr-code": "^4.3",
        "illuminate/contracts": "^8.37",
        "illuminate/support": "^8.37",
        "intervention/image": "^2.7",
        "ksubileau/color-thief-php": "^1.4",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "graham-campbell/analyzer": "^3.0",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-parallel": "^0.3.1",
        "spatie/laravel-ray": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "Faust\\CharacterBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage --parallel",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Faust\\CharacterBuilder\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
